msgid "left thumb"
msgstr "kōnui mauī"

#: lessonbuilder.py:287
msgid "right little"
msgstr "kōiti matau"

#: lessonbuilder.py:288
msgid "right ring"
msgstr "manawa matau"

#: lessonbuilder.py:289
msgid "right middle"
msgstr "māpere matau"

#: lessonbuilder.py:290
msgid "right index"
msgstr "kōroa matau"

#: lessonbuilder.py:291 lessonscreen.py:48
msgid "right thumb"
