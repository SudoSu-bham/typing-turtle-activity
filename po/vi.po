msgstr "<span font_desc='Sans 16'>trong <b><u><i>%(lesson)s</i></u></b></span>"

#: medalscreen.py:69
#, python-format
msgid "<span font_desc='Sans 16'>on <b><u><i>%(date)s</i></u></b>.</span>"
msgstr "<span font_desc='Sans 16'>vào <b><u><i>%(date)s</i></u></b>.</span>"

